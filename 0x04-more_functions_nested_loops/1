#include <stdio.h>
#include "main.h"

/**
* main - print FizzBuzz
* Return - 0 SUCCESS
*/

int main(void)
{
	int i;

	for (i = 0; i < 100; i++)
	{
		if (i % 3 != 0 && i % 5 != 0)
		{
			printf("%d", i);
		}
		else
		{
			if (i % 3 == 0)	
				printf("Fizz");
			if (i % 5 == 0)
				printf("Buzz");
		}

	}
	return (0);
}
